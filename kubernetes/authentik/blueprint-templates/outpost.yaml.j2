---
version: 1

metadata:
  name: outpost-config

entries:

# Outpost Configuration
- identifiers:
    name: authentik Embedded Outpost
  model: authentik_outposts.outpost
  state: present
  attrs:
    providers:
{%- for app_name in app_names %}
    - !Find
      - authentik_providers_proxy.proxyprovider
      - [name, {{ app_name }}]
{%- endfor %}
