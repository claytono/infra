{
  "resource": {
    "authentik_application": {
      "alertmanager": {
        "name": "alertmanager",
        "protocol_provider": "${authentik_provider_proxy.alertmanager.id}",
        "slug": "alertmanager"
      }
    },
    "authentik_provider_proxy": {
      "alertmanager": {
        "access_token_validity": "hours=1",
        "authorization_flow": "${data.authentik_flow.default_authorization.id}",
        "cookie_domain": "oneill.net",
        "external_host": "https://alertmanager.k.oneill.net",
        "internal_host": "http://alertmanager.default.svc.cluster.local:9093",
        "invalidation_flow": "${data.authentik_flow.default_invalidation.id}",
        "mode": "forward_domain",
        "name": "alertmanager"
      }
    }
  }
}
