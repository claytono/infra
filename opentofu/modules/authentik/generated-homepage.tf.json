{
  "resource": {
    "authentik_application": {
      "homepage": {
        "name": "homepage",
        "protocol_provider": "${authentik_provider_proxy.homepage.id}",
        "slug": "homepage"
      }
    },
    "authentik_provider_proxy": {
      "homepage": {
        "access_token_validity": "hours=1",
        "authorization_flow": "${data.authentik_flow.default_authorization.id}",
        "cookie_domain": "oneill.net",
        "external_host": "https://homepage.k.oneill.net",
        "internal_host": "http://homepage.default.svc.cluster.local:3000",
        "invalidation_flow": "${data.authentik_flow.default_invalidation.id}",
        "mode": "forward_domain",
        "name": "homepage"
      }
    }
  }
}
