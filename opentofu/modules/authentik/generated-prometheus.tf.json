{
  "resource": {
    "authentik_application": {
      "prometheus": {
        "name": "prometheus",
        "protocol_provider": "${authentik_provider_proxy.prometheus.id}",
        "slug": "prometheus"
      }
    },
    "authentik_provider_proxy": {
      "prometheus": {
        "access_token_validity": "hours=1",
        "authorization_flow": "${data.authentik_flow.default_authorization.id}",
        "cookie_domain": "oneill.net",
        "external_host": "https://prometheus.k.oneill.net",
        "internal_host": "http://prometheus.default.svc.cluster.local:9090",
        "invalidation_flow": "${data.authentik_flow.default_invalidation.id}",
        "mode": "forward_domain",
        "name": "prometheus"
      }
    }
  }
}
