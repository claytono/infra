{
  "resource": {
    "authentik_application": {
      "prowlarr": {
        "name": "prowlarr",
        "protocol_provider": "${authentik_provider_proxy.prowlarr.id}",
        "slug": "prowlarr"
      }
    },
    "authentik_provider_proxy": {
      "prowlarr": {
        "access_token_validity": "hours=1",
        "authorization_flow": "${data.authentik_flow.default_authorization.id}",
        "basic_auth_enabled": true,
        "basic_auth_password_attribute": "password",
        "basic_auth_username_attribute": "username",
        "cookie_domain": "oneill.net",
        "external_host": "https://prowlarr.k.oneill.net",
        "internal_host": "http://prowlarr.default.svc.cluster.local:9696",
        "invalidation_flow": "${data.authentik_flow.default_invalidation.id}",
        "mode": "forward_domain",
        "name": "prowlarr",
        "skip_path_regex": "^https://prowlarr.k.oneill.net/\\d+/api.*\n^https://prowlarr.k.oneill.net/api.*\n^https://prowlarr.k.oneill.net/\\d+/download.*"
      }
    }
  }
}
