{
  "resource": {
    "authentik_application": {
      "qbit-manage": {
        "name": "qbit-manage",
        "protocol_provider": "${authentik_provider_proxy.qbit-manage.id}",
        "slug": "qbit-manage"
      }
    },
    "authentik_provider_proxy": {
      "qbit-manage": {
        "access_token_validity": "hours=1",
        "authorization_flow": "${data.authentik_flow.default_authorization.id}",
        "cookie_domain": "oneill.net",
        "external_host": "https://qbit-manage.k.oneill.net",
        "internal_host": "http://qbit-manage.default.svc.cluster.local:8080",
        "invalidation_flow": "${data.authentik_flow.default_invalidation.id}",
        "mode": "forward_domain",
        "name": "qbit-manage"
      }
    }
  }
}
