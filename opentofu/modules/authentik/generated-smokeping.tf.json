{
  "resource": {
    "authentik_application": {
      "smokeping": {
        "name": "smokeping",
        "protocol_provider": "${authentik_provider_proxy.smokeping.id}",
        "slug": "smokeping"
      }
    },
    "authentik_provider_proxy": {
      "smokeping": {
        "access_token_validity": "hours=1",
