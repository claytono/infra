{
  "resource": {
    "authentik_application": {
      "sonarr": {
        "name": "sonarr",
        "protocol_provider": "${authentik_provider_proxy.sonarr.id}",
        "slug": "sonarr"
      }
    },
    "authentik_provider_proxy": {
      "sonarr": {
        "access_token_validity": "hours=1",
        "authorization_flow": "${data.authentik_flow.default_authorization.id}",
        "basic_auth_enabled": true,
        "basic_auth_password_attribute": "password",
        "basic_auth_username_attribute": "username",
        "cookie_domain": "oneill.net",
        "external_host": "https://sonarr.k.oneill.net",
        "internal_host": "http://sonarr.default.svc.cluster.local:8989",
        "invalidation_flow": "${data.authentik_flow.default_invalidation.id}",
        "mode": "forward_domain",
        "name": "sonarr",
        "skip_path_regex": "^https://sonarr.k.oneill.net/api/v\\d+.*"
      }
    }
  }
}
